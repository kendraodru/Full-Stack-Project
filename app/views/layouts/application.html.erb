<!DOCTYPE html>
<html>
  <head>
    <title>YouthToThePeople</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type='text/javascript'> 
      window.favicon = "<%= image_url('favicon.png') %>";
    </script>
    <link rel='shortcut icon' type="image/png" href="<%= image_url('favicon.png')%>">
    <link href="https://fonts.googleapis.com/css?family=Dancing+Script|Spartan&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/998bf06365.js" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/b964b5d871.js" crossorigin="anonymous"></script>

    <meta property='og:title' content='Youth To The People'/>
    <meta property='og:image' content="<%= image_url('yttpdemo.png') %>"/>
    <meta property='og:description' content='Better Skin Starts Here'/>
    <meta property='og:url' content='https://youth-to-the-people.herokuapp.com/' />
  
  </head>

  <body>
    <%= yield %>
  </body>
</html>
